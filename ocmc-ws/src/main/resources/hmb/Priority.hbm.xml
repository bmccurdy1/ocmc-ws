<?xml version="1.0"?>  
 <!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
 <hibernate-mapping>  
   <class name="com.ocmc.ws.beans.PriorityBean" table="priority">  
     <id name="priorityID" column="priorityID" type="long" access="field">  
       <generator class="increment" />  
     </id>  
     <property name="priorityName" type="string">  
       <column name="priorityName" />  
     </property>  
     <property name="priorityDesc" type="string">  
       <column name="priorityDesc" />  
     </property>  
   </class>  
 </hibernate-mapping>  